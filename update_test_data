!/bin/bash
# Update the test data from Unicode
wget https://www.unicode.org/Public/UCD/latest/ucd/auxiliary/GraphemeBreakTest.txt -O ./test/support/GraphemeBreakTest.txt
wget https://www.unicode.org/Public/UCD/latest/ucd/auxiliary/WordBreakTest.txt -O ./test/support/WordBreakTest.txt
wget https://www.unicode.org/Public/UCD/latest/ucd/auxiliary/SentenceBreakTest.txt -O ./test/support/SentenceBreakTest.txt
wget https://www.unicode.org/Public/UCD/latest/ucd/auxiliary/LineBreakTest.txt -O ./test/support/LineBreakTest.txt


